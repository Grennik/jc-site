<div class="container-liquid" id="admin-dash">
<% if admin_signed_in? %>
  
  <%= link_to('Logout', destroy_admin_session_path, :method => :delete) %>        
  
<% else %>
 
  <%= link_to('Login', new_admin_session_path)  %>  
  
<% end %>
</div>
<div class="container" id="admin-body">
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <h1>Projects Listing</h1>
     <%= form_for (@page) do |f| %>
     <p>
     <%= f.label "Project Title" %><br>
     <%= f.text_area :name %>
     </p>
     <p>
     <%= f.label "Description" %><br>
     <%= f.text_area :description %>
     </p>
     <% end %>
  </div>
</div>